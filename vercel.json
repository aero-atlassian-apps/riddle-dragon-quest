{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "functions": {
        "api/supabase-proxy.ts": {
            "maxDuration": 30
        }
    },
    "buildCommand": "npm run build",
    "installCommand": "rm -rf node_modules && rm -f package-lock.json && npm install"
}