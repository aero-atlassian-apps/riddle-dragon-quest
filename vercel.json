{
    "rewrites": [
        {
            "source": "/api/supabase/rest/v1/(.*)",
            "destination": "/api/proxy/rest/$1"
        },
        {
            "source": "/api/supabase/auth/v1/(.*)",
            "destination": "/api/proxy/auth/$1"
        },
        {
            "source": "/api/supabase/storage/v1/(.*)",
            "destination": "/api/proxy/storage/$1"
        },
        {
            "source": "/api/supabase/realtime/v1/(.*)",
            "destination": "/api/proxy/realtime/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "buildCommand": "npm run build",
    "installCommand": "rm -rf node_modules && rm -f package-lock.json && npm install"
}